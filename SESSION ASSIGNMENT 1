//ASSIGNMENT 1

#include <iostream>
using namespace std;
int main()
{
    int a,num;
    cout<<"Enter the number : \n";
    cin>>num;

    while(num>0)
    {
        a=num%10;
        
        if(a!=1 && a!=0)
        {
            cout<<"Number is NOT Binary \n";
            break;

        }
        else
        {
            cout<<"Number is Binary \n";
            break;
        }
    }
 return 0;   
}
